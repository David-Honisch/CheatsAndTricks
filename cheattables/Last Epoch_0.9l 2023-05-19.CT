alloc(newmem,256,hkDEOH)
label(hkDEOHbkp)
label(return)
label(SetDamage)
label(CleanUp)
label(isPlayerOrMinion)
registersymbol(hkDEOH)
registersymbol(hkDEOHbkp)
newmem:
  cmp  rcx, BAD_PTR
  jle  hkDEOHbkp
  push rbx
  mov  rbx, [rbx+0178]
  cmp  rbx, [PlayerActor]
  je   CleanUp
  mov  rbx, [rcx+038]
  test rbx, rbx
  je   isPlayerOrMinion
  mov  rbx, [rbx+018]
  cmp  rbx, BAD_PTR
  jle  CleanUp
  cmp  BYTE PTR [rbx+0185], 0
  jne  isPlayerOrMinion
  cmp  BYTE PTR [rbx+0209], 0
  jne  CleanUp
isPlayerOrMinion:
  mov  rbx, [rcx+018]
  cmp  rbx, BAD_PTR
  jle  @F
  call SetDamage
@@:
  mov  rbx, [rcx+020]
  cmp  rbx, BAD_PTR
  jle  @F
  push eax
  mov  eax, [optAddedDamageScaling]
  mov  [rbx+038], eax
  pop  eax
  call SetDamage
@@:
  push eax
  mov  eax, [optDamageModifier]
  mov  [rcx+02C], eax
  pop  eax
CleanUp:
  pop  rbx
hkDEOHbkp:
  readmem(hkDEOH,5)
  jmp return
ALIGN 10 CC
SetDamage:
  push eax
  mov  BYTE PTR [rbx+020], 1
  mov  eax, [optCritChange]
  mov  [rbx+018], eax
  mov  eax, [optCritMultiplier]
  mov  [rbx+01C], eax
  mov  eax, [optAdditionalLeech]
  mov  [rbx+03C], eax
{
  mov  rax, [optFreezeRate]
  mov  [rbx+040], rax
  mov  rax, [optIncreasedStunChance]
  mov  [rbx+034], rax
  mov  rax, [optIncreasedStunDuration]
  mov  [rbx+038], rax
}
  pop  eax
  ret
hkDEOH:
  jmp newmem
return:
[DISABLE]
hkDEOH:
  readmem(hkDEOHbkp,5)
unregistersymbol(*)
dealloc(*)
