  if(getCEVersion() &lt; 7.2) then
    ShowMessage('Cheat Engine 7.2 or above is desirable.')
  end
end

function Menu()
  if Exist then return end
  local parent = getMainForm().Menu.Items
  Item = createMenuItem(parent); parent.add(Item)
  Item.Caption = 'Compact View Mode'
  Item.OnClick = Routine
  Exist = 'yes'
end

function Routine(sender,force)
  local State = not(Item.Caption == 'Compact View Mode')
  if force ~= nil then State = not force end
  Item.Caption = State and 'Compact View Mode' or 'Full View Mode'
  getMainForm().Splitter1.Visible = State
  getMainForm().Panel4.Visible    = State
  getMainForm().Panel5.Visible    = State
end

PROCESS_NAME = 'ff7remake_.exe'
local Attach = nil
local Interval = 100
local MinCount = 0
local MaxCount = 1000

local function AutoAttach(Timer)
    if getProcessIDFromProcessName(PROCESS_NAME) == nil then return end
        Timer.destroy()
        Reaction = messageDialog('Attach to Process?',mtConfirmation,mbYes,mbNo)
        if Reaction == mrYes then
        openProcess(PROCESS_NAME)
        Routine(nil,true)
        elseif MaxCount &gt; 0 and MinCount &gt;= MaxCount then
        Timer.destroy()
    end
    MinCount = MinCount + 1
end

CheckVersion()
Menu()
Attach = createTimer(MainForm)
Attach.Interval = Interval
Attach.OnTimer = AutoAttach
</LuaScript></CheatTable>