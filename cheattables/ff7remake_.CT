aobscanmodule(Get_New_Amount,ff7remake_.exe,41898CC74C560300)
alloc(Alloc_GNA,$128,Get_New_Amount)
alloc(mem_GNA,8)
label(Extend return)
registersymbol(Get_New_Amount mem_GNA)

mem_GNA:
  reassemble(Get_New_Amount)

Alloc_GNA:
  cmp [r15+rax*8+00035650],8
  je Extend
  cmp [r15+rax*8+00035650],9
  je Extend
  reassemble(Get_New_Amount)
  jmp return

Extend:
  mov ecx,4
  reassemble(Get_New_Amount)
  jmp return

Get_New_Amount:
  jmp Alloc_GNA
  nop 3
return:

[DISABLE]
Get_New_Amount:
