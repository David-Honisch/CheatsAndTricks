label(_retlife)
label(_retmoney)
label(_retxp)
label(_retfurymeter)
label(_retskillpoints)

label(stamjmp)
label(lifejmp)
label(moneyjmp)
label(xpjmp)
label(furymeterjmp)
label(skillpointsjmp)

