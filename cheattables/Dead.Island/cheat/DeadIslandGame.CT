aobscan(_money,8B 82 xx xx xx xx 8D 4C 24 xx 51 xx xx xx xx xx 83 C4 08)
aobscan(_xp,8B 78 xx 2B 78 xx 8B CB FF D6 84 C0 74 07)
aobscan(_furymeter,D9 9F D0 12 00 00 E8 91 59 00 00 83 F8 02)
aobscan(_skillpoints,D9 40 04 0F B7 44 24 02 0D 00 0C 00 00 89 44 24 04 53)

//Skill points
_memskillpoints:
push eax
lea eax,[eax+04]
mov [skillpoints],eax
pop eax
fld dword ptr [eax+04]
movzx eax,word ptr [esp+02]
jmp _retskillpoints

_skillpoints:
skillpointsjmp:
jmp _memskillpoints
nop
nop
nop
_retskillpoints:

//Fury Meter
_memfurymeter:
push eax
lea eax,[edi+000012D0]
mov [furymeter],eax
pop eax
fstp dword ptr [edi+000012D0]
jmp _retfurymeter

_furymeter:
furymeterjmp:
jmp _memfurymeter
nop
_retfurymeter:

//XP
_memxp:
push esi
lea esi,[eax+3C]
mov [xp],esi
pop esi
mov edi,[eax+3C]
sub edi,[eax+40]
jmp _retxp

_xp:
xpjmp:
jmp _memxp
nop
_retxp:

//Money
_memmoney:
push eax
lea eax,[edx+00000778]
mov [money],eax
pop eax
mov eax,[edx+00000778]
jmp _retmoney

_money:
moneyjmp:
jmp _memmoney
nop
_retmoney:

//Stamina
_memstam:
push eax
lea eax,[ebx+0000091C]
mov [stamina],eax
pop eax
fst dword ptr [ebx+0000091C]
jmp _retstam

_stamina:
stamjmp:
jmp _memstam
nop
_retstam:

//Life
_memlife:
push eax
lea eax,[esi+000008B8]
mov [life],eax
pop eax
fcomp dword ptr [esi+000008B8]
jmp _retlife

_life:
lifejmp:
jmp _memlife
nop
_retlife:

[Disable]
skillpointsjmp:
fld dword ptr [eax+04]
movzx eax,word ptr [esp+02]

furymeterjmp:
fstp dword ptr [edi+000012D0]

xpjmp:
mov edi,[eax+3C]
sub edi,[eax+40]

stamjmp:
fst dword ptr [ebx+0000091C]

lifejmp:
fcomp dword ptr [esi+000008B8]

moneyjmp:
mov eax,[edx+00000778]

dealloc(_memstam)
dealloc(_memlife)
dealloc(_memmoney)
dealloc(_memxp)
dealloc(_memfurymeter)
dealloc(_memskillpoints)
unregistersymbol(stamjmp)
unregistersymbol(lifejmp)
unregistersymbol(moneyjmp)
unregistersymbol(xpjmp)
unregistersymbol(furymeterjmp)
unregistersymbol(skillpointsjmp)
</AssemblerScript>
