  <CheatEntries>
    <CheatEntry>
      <Description>"Infinite Durability (Equipped Item)"</Description>
      <Color>80000008</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[Enable]
alloc(newmem,256)
label(returnhere)
label(durajmp)
registersymbol(durajmp)
aobscan(_durability,D9 47 24 8B 90 C4 02 00 00 D9 5C xx xx 8B CE )
newmem:
mov [edi+24],(float)999
fld dword ptr [edi+24]
mov edx,[eax+000002C4]
jmp returnhere

_durability:
durajmp:
jmp newmem
nop
nop
nop
nop
returnhere:

[Disable]
durajmp:
fld dword ptr [edi+24]
mov edx,[eax+000002C4]
dealloc(newmem)
unregistersymbol(durajmp)
</AssemblerScript>
    </CheatEntry>
    <CheatEntry>
      <Description>"Instant Fury after 1 kill"</Description>
      <Color>80000008</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[Enable]
alloc(newmem,256)
label(returnhere)
label(furyjmp)
registersymbol(furyjmp)

aobscan(_fury,D9 9E D0 12 00 00 5F 5B)

newmem:
mov [esi+000012D0],(float)1.0

jmp returnhere

_fury:
furyjmp:
jmp newmem
nop
returnhere:

[Disable]

furyjmp:
fstp dword ptr [esi+000012D0]

unregistersymbol(furyjmp)
dealloc(newmem)
</AssemblerScript>
    </CheatEntry>
    <CheatEntry>
      <Description>"Get xx item on drop"</Description>
      <Color>80000008</Color>
      <VariableType>Auto Assembler Script</VariableType>
      <AssemblerScript>[Enable]
alloc(newmem,256)
globalalloc(itemamt,4)
label(returnhere)
label(itemdec)
label(itemdec2)
label(itemdec3)

registersymbol(itemdec)
registersymbol(itemdec2)
registersymbol(itemdec3)

aobscan(_itemdec,FF 4E 20 56 56 55)

newmem:
push eax
mov eax,[itemamt]
mov [esi+20],eax
pop eax
push esi
push esi
jmp returnhere

_itemdec:
itemdec:
jmp newmem
returnhere:

_itemdec-2:
itemdec2:
nop
nop

_itemdec-14:
itemdec3:
nop
nop

itemamt:
db 63

[disable]
itemdec:
dec [esi+20]
push esi
push esi

itemdec2:
db 7E 1B

itemdec3:
db 7E 2D

itemamt:
db 00

dealloc(newmem)
unregistersymbol(itemdec)
unregistersymbol(itemdec2)
unregistersymbol(itemdec3)

</AssemblerScript>
      <CheatEntries>
