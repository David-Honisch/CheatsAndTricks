function TableOptionsCounter()
  local al = AddressList
  local _Record,_Hotkey,_Header = 0, 0, 0
  local _Script,_Total = 0, al.count
        for i = 0, al.Count -1 do
            if  al[i].Type == vtAutoAssembler then
                _Script = _Script +1
            end
            if  al[i].Type ~= vtAutoAssembler and
                al[i].IsGroupHeader == false then
			   _Record = _Record +1
            end
            if al[i].Type ~= vtAutoAssembler and
               al[i].IsGroupHeader == true then
			   _Header = _Header +1
            end
            if al[i].HotkeyCount &gt; 0 then
               _Hotkey = _Hotkey +1
            end
        end
        showMessage('Script      '.._Script..
                    '\nHeader   '.._Header..
                    '\nRecord    '.._Record..
                    '\n\nTotal        '.._Total..
                    '\n\nHotkey    '.._Hotkey)
end

