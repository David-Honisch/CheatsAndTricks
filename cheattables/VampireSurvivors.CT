         local al = AddressList
         for i = 0, al.count -1 do
             if al[i].Type == 0 and
                al[i].Parent == nil and
                al[i].Description ~= "Basic" and
                al[i].isAddressGroupHeader == false then
                al[i].Active = true
             end
         end
end

function Check()
  if(getCEVersion() &lt; 7.4) then
    ShowMessage('Cheat Engine 7.4 or above is recommended.')
  end
end

function Compact()
  if CV_Exists then return end
  local parent = getMainForm().Menu.Items
  mi = createMenuItem(parent); parent.add(mi)
  mi.Caption = 'Compact'
  mi.OnClick = Routine
  CV_Exists = 'yes'
end

function Routine(sender,force)
  local state = not(mi.Caption == 'Compact')
  local mf = getMainForm()
  if force ~= nil then state = not force end
  mi.Caption = state and 'Compact' or 'Full View'
  mf.Splitter1.Visible = state
  mf.Panel4.Visible    = state
  mf.Panel5.Visible    = state
end

PROCESS_NAME = 'VampireSurvivors.exe'
local Attach = nil
local Interval = 100
local MinCount = 0
local MaxCount = 1000

local function AutoAttach(Timer)
    if getProcessIDFromProcessName(PROCESS_NAME) == nil then return end
        Timer.destroy()
        openProcess(PROCESS_NAME)
        mono_initialize()
        LaunchMonoDataCollector()
        Routine(nil,true)
        if MaxCount &gt; 0 and MinCount &gt;= MaxCount then
        Timer.destroy()
    end
    MinCount = MinCount + 1
end

Open()
Check()
Compact()
Attach = createTimer(MainForm)
Attach.Interval = Interval
Attach.OnTimer = AutoAttach
