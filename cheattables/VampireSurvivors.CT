{$lua}
if readInteger(process) == nil or syntaxcheck then return end

local StartID = allocateMemory(4)
local EndID = allocateMemory(4)
registerSymbol("Start_ID", StartID)
registerSymbol("End_ID", EndID)

local al = getAddressList()
local Count = 0

function CheckParent()
    for i = 0, al.Count - 1 do
        if al[i].Description == "DropdownList" then
            ParentNum = al[i]
        end
    end
end

function createSERecords(Header, Address, Description, Type)
    local mr = al.createMemoryRecord()
    mr.appendToEntry(Header)
    mr.Address = Address
    mr.Description = Description
    mr.Type = Type
    mr.color = 0x000080
end

function SetStartEnd()
    for i = 0, al.Count - 1 do
        if al[i].Parent == ParentNum then
            Count = al[i].ID
        end
    end
    createSERecords(memrec, StartID, "Start ID", vtDword)
    createSERecords(memrec, EndID, "End ID", vtDword)
    writeInteger(StartID, Count + 0x02)
    writeInteger(EndID, al.Count)
end

CheckParent()
SetStartEnd()

