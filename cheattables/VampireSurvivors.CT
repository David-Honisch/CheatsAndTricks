function Routine(sender,force)
  local state = not(mi.Caption == 'Compact')
  local mf = getMainForm()
  if force ~= nil then state = not force end
  mi.Caption = state and 'Compact' or 'Full View'
  mf.Splitter1.Visible = state
  mf.Panel4.Visible    = state
  mf.Panel5.Visible    = state
end

function Compact()
  if CV_Exists then return end
  local parent = getMainForm().Menu.Items
  mi = createMenuItem(parent); parent.add(mi)
  mi.Caption = 'Compact'
  mi.OnClick = Routine
  CV_Exists = 'yes'
end

Compact()
