function OpenFolder(Path)                                                               -- Open the folder at fiven path in the Windows Explorer
    local Command = "explorer \"" .. Path .. "\""
    os.execute(Command)
end

local Process_Path = enumModules()[1].PathToFile                                        -- Get the path of first module in attached process

function Get_Path(Path)                                                                 -- Extract the directory path from the given file path
  local Temporary_Path = Path
  if Temporary_Path == nil then return end
  for Directory in Temporary_Path:gmatch('(.-)([^\\]-([^%.]+))$') do                    -- The pattern is used to match the directory path at the beginning of the string up to the last occurrence of a path separator
    return Directory
  end
end

local Default_Path = Get_Path(Process_Path) .. ''

local Reaction = messageDialog("Open the game folder?", mtConfirmation, mbYes, mbNo )   -- Display message box asking the user if they want to open the game folder

if Reaction == mrYes then
  if Default_Path:find("Cheat Engine") then                                             -- Find to check if the substring Cheat Engine exists in the string
    messageDialog("Must be attached to the target process.",mtError,mbOK)               -- If the above checks are caught, send out a warning
  else
    OpenFolder(Default_Path)
  end
end

return 'nop'

