function OptionsTypeCounter()
  local al = AddressList
  local _Byte,_Word,_Dword,_Qword = 			 	   0, 0, 0, 0
  local _Single,_Double,_String,_UnicodeString =       0, 0, 0, 0
  local _ByteArray,_Binary,_All,_Pointer = 		       0, 0, 0, 0
  local _Custom,_Grouped,_ByteArrays,_CodePageString = 0, 0, 0, 0
  local _AutoAssembler,_Header,_Total =                0, 0, al.count
        for i = 0, al.Count -1 do
            if  al[i].Type == 0x00 and al[i].IsGroupHeader == false then _Byte =  _Byte +1           end
            if  al[i].Type == 0x01 then _Word =                                   _Word +1           end
            if  al[i].Type == 0x02 then _Dword =                                  _Dword +1          end
            if  al[i].Type == 0x03 then _Qword =                                  _Qword +1          end
            if  al[i].Type == 0x04 then _Single =                                 _Single +1         end
            if  al[i].Type == 0x05 then _Double =                                 _Double +1         end
            if  al[i].Type == 0x06 then _String =                                 _String +1         end
            if  al[i].Type == 0x07 then _UnicodeString =                          _UnicodeString +1  end
            if  al[i].Type == 0x08 then _ByteArray =                              _ByteArray +1      end
            if  al[i].Type == 0x09 then _Binary =                                 _Binary +1         end
            if  al[i].Type == 0x0A then _All =                                    _All +1            end
            if  al[i].Type == 0x0B then _AutoAssembler =                          _AutoAssembler +1  end
            if  al[i].Type == 0x0C then _Pointer =                                _Pointer +1        end
            if  al[i].Type == 0x0D then _Custom =                                 _Custom +1         end
            if  al[i].Type == 0x0E then _Grouped =                                _Grouped +1        end
            if  al[i].Type == 0x0F then _ByteArrays =                             _ByteArrays +1     end
            if  al[i].Type == 0x10 then _CodePageString =                         _CodePageString +1 end
            if  al[i].Type == 0x00 and al[i].IsGroupHeader == true then _Header = _Header +1         end
        end
        showMessage('Byte     　  　　  '.._Byte..'\nWord　   　　　'.._Word..'\nDword   　  　　'.._Dword..'\nQword        　　 '.._Qword..'\nSingle     　　　 '.._Single..'\nDouble    　　　'.._Double..'\nString 　　　　 '.._String..'\nUnicodeString    '.._UnicodeString..'\nByteArray　　    '.._ByteArray..'\nBinary    　　　  '.._Binary..'\nAll 　　　　　   '.._All..'\nPointer  　　　  '.._Pointer..'\nCustom   　　　 '.._Custom..'\nGrouped   　　   '.._Grouped..'\nByteArrays           '.._ByteArrays..'\nCodePageString  '.._CodePageString..'\nAutoAssembler   '.._AutoAssembler..'\nHeader     　　　'.._Header..'\nTotal    　　　　  '.._Total)
end

