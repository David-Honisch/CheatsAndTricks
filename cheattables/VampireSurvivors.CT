function Attach_mono()
  local PID = getOpenedProcessID()
  local GP = "VampireSurvivors.exe"
  local TPID = getProcessIDFromProcessName(GP)

  if PID ~= TPID then
    if TPID ~= nil then
      openProcess(TPID)
       mono_initialize()
       LaunchMonoDataCollector()
    else
      showMessage("Failed to find process: " .. GP)
    end
  end
end

