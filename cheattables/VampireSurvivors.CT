function TableOptionsCounter_2()
  local al = getAddressList()
  local StartID = readInteger("Start_ID")
  local EndID = readInteger("End_ID")

  if StartID == nil then
    showMessage("Set IDs before executing")
    return
  end

  local Total = EndID - StartID + 1
  local _Record, _Hotkey, _Header, _Script = 0, 0, 0, 0

  for i = 0, al.Count - 1 do
    local Num = al[i]
    if Num.ID &gt;= StartID and Num.ID &lt;= EndID then
      if Num.Type == vtAutoAssembler then
        _Script = _Script + 1
      elseif not Num.IsGroupHeader then
        _Record = _Record + 1
      elseif Num.IsGroupHeader then
        _Header = _Header + 1
      end

      if Num.HotkeyCount &gt; 0 then
        _Hotkey = _Hotkey + 1
      end
    end
  end

  showMessage('Script      '.._Script..
              '\nHeader   '.._Header..
              '\nRecord    '.._Record..
              '\n\nTotal        '..Total..
              '\n\nHotkey    '.._Hotkey)
end

TableOptionsCounter_2()
return 'nop'

